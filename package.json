{
  "name": "fivem-frontend-lib",
  "version": "1.0.0",
  "description": "Biblioteca para desenvolvimento front-end para FiveM",
  "main": "src/index.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts",
    "test": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marquezenrique/fivem-frontend-lib.git"
  },
  "keywords": [
    "fivem",
    "lib",
    "frontend"
  ],
  "author": "Enrique Marquez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/marquezenrique/fivem-frontend-lib/issues"
  },
  "homepage": "https://github.com/marquezenrique/fivem-frontend-lib#readme",
  "dependencies": {
    "react": "19.1.1",
    "tsup": "^8.5.0"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "typescript": "^5.9.3"
  }
}
